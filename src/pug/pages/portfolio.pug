- var portfolio_routes = require('../../js/configurations/portfolio_routes.json');
#portfolio.page(data-title="portfolio")
    section.page-content
        .portfolio-items-wrapper
            .portfolio-scrolling-block
                each val, index in portfolio_routes
                    a.portfolio-item(href=val, id=val, data-text=index)=index
                        .portfolio-item-title=index
                        div(id=index).page
                            header.header.page-header
                                h1.header-title The Nodio
                                p.header-text Page with modern style and smart-grid.
                            section.page-content





